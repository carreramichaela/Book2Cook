<h1> I am the books page </h1>

  <table class="table">
  <tbody>
  <% @books.each do |book| %>
      <tr>
      <td> <%= link_to book.name, book %> </td>
        <td>
          <div class="btn-group float-right">
          <%= link_to 'Delete',
                book,
                method: :delete,
                data: {confirm: 'Are you sure?' },
                class: "btn btn-danger" %>
          </div>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
